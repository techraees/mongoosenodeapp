{
  "version": 2,
  "builds": [
    { "src": "*.js", "use": "@vercel/node" },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    },
{
"src": "/public/(.)",
"dest": "/public/$1"
},
{
"src": "/(.*)",
"dest": "/index.js"
}
  ]
}
